{
  "name": "gss2022-frontend",
  "description": "Suikoden Election 2022 Frontend",
  "version": "0.1.0",
  "dependencies": {
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@bugsnag/js": "7.17.4",
    "@bugsnag/plugin-react": "7.17.4",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/node": "17.0.23",
    "@types/react": "18.0.20",
    "@types/react-burger-menu": "2.8.3",
    "@types/react-copy-to-clipboard": "5.0.4",
    "@types/styled-components": "5.1.26",
    "autoprefixer": "10.4.11",
    "axios": "0.27.2",
    "cypress": "10.8.0",
    "daisyui": "2.28.0",
    "dayjs": "1.11.5",
    "eslint": "8.23.1",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "8.5.0",
    "gridjs": "5.1.0",
    "gridjs-react": "5.1.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "next-translate": "1.6.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "react-burger-menu": "3.0.8",
    "react-copy-to-clipboard": "5.1.0",
    "react-twitter-embed": "4.0.4",
    "sass": "1.54.9",
    "sharp": "0.31.0",
    "styled-components": "5.3.5",
    "stylelint": "14.12.0",
    "stylelint-config-standard": "28.0.0",
    "stylelint-order": "5.0.0",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.3",
    "wait-on": "6.0.1"
  },
  "main": "index.ts",
  "private": true,
  "scripts": {
    "build": "next build",
    "cypress:exec": "run-s cypress:server:kill & run-s cypress:server:run & run-s cypress:server:wait-on && run-s cypress:run && run-s cypress:server:kill",
    "cypress:run": "cypress run --browser chrome",
    "cypress:server:kill": "kill $(lsof -i :3100 | grep node | awk '{print $2}')",
    "cypress:server:run": "next build && next start --port 3100",
    "cypress:server:wait-on": "wait-on -t 30000 http-get://localhost:3100",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start"
  }
}
