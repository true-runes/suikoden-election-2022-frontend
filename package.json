{
  "name": "gss2022-frontend",
  "description": "Suikoden Election 2022 Frontend",
  "version": "0.1.0",
  "dependencies": {
    "next": "12.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@bugsnag/js": "7.21.0",
    "@bugsnag/plugin-react": "7.19.0",
    "@testing-library/jest-dom": "6.1.1",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.17.8",
    "@types/react": "18.0.26",
    "@types/react-burger-menu": "2.8.3",
    "@types/react-copy-to-clipboard": "5.0.4",
    "@types/styled-components": "5.1.26",
    "autoprefixer": "10.4.15",
    "axios": "1.4.0",
    "cypress": "12.17.4",
    "daisyui": "3.6.1",
    "dayjs": "1.11.9",
    "eslint": "8.47.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "9.0.0",
    "gridjs": "6.0.6",
    "gridjs-react": "6.0.1",
    "jest": "29.6.3",
    "jest-environment-jsdom": "29.6.3",
    "next-translate": "1.6.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.28",
    "prettier": "3.0.2",
    "react-burger-menu": "3.0.9",
    "react-copy-to-clipboard": "5.1.0",
    "react-twitter-embed": "4.0.4",
    "sass": "1.66.1",
    "sharp": "0.32.5",
    "styled-components": "6.0.7",
    "stylelint": "15.10.3",
    "stylelint-config-standard": "34.0.0",
    "stylelint-order": "6.0.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6",
    "wait-on": "7.0.1"
  },
  "main": "index.ts",
  "private": true,
  "scripts": {
    "build": "next build",
    "cypress:exec": "run-s cypress:server:kill & run-s cypress:server:run & run-s cypress:server:wait-on && run-s cypress:run && run-s cypress:server:kill",
    "cypress:run": "cypress run --browser chrome",
    "cypress:server:kill": "kill $(lsof -i :3100 | grep node | awk '{print $2}')",
    "cypress:server:run": "next build && next start --port 3100",
    "cypress:server:wait-on": "wait-on -t 30000 http-get://localhost:3100",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start"
  }
}
